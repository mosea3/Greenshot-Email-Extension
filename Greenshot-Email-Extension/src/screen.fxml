<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<BorderPane minHeight="200.0" minWidth="300.0" prefHeight="200.0" xmlns="http://javafx.com/javafx/8.0.191" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <top>
      <BorderPane maxHeight="0.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <center>
            <BorderPane BorderPane.alignment="CENTER">
               <right>
                  <Button fx:id="SendButton" minWidth="60.0" mnemonicParsing="false" onAction="#SendButtonPressed" text="senden" BorderPane.alignment="CENTER" />
               </right>
               <center>
                  <TextField fx:id="emailInput" minWidth="240.0" BorderPane.alignment="CENTER" />
               </center>
            </BorderPane>
         </center>
         <top>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Emailadresse eingeben" BorderPane.alignment="CENTER" />
         </top>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </BorderPane>
   </top>
   <center>
      <ListView id="listView" fx:id="listView" prefHeight="144.0" prefWidth="300.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
